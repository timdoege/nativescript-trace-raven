{
    "name": "nativescript-trace-raven",
    "version": "1.1.1",
    "description": "Custom NativeScript TraceWriter using Raven to write logs to Sentry.io",
    "typings": "index.d.ts",
    "nativescript": {
        "platforms": {
            "android": "6.0.0",
            "ios": "6.0.0"
        }
    },
    
    "main": "src/index.js",
    "files": [
        "src/*",
        "!src/node_modules",
        "!src/references.d.ts",
        "!src/tsconfig.json"
    ],
    "scripts": {
        "prepare": "npm run build",
        "build": "cd ./src && npm run build"
    },

    "repository": {
      "type": "git",
      "url": "https://github.com/toddanglin/nativescript-trace-raven.git"
    },
    "keywords": [
      "NativeScript",
      "JavaScript",
      "Android",
      "iOS",
      "Sentry",
      "Raven",
      "logging"
    ],
    "author": {
      "name": "Todd Anglin",
      "email": "toddanglin@gmail.com"
    },
    "bugs": {
      "url": "https://github.com/toddanglin/nativescript-trace-raven/issues"
    },
    "license": "MIT",
    "homepage": "https://github.com/toddanglin/nativescript-trace-raven",
    "readmeFilename": "README.md",
    "devDependencies": {
      "tns-core-modules": "^6.1.1",
      "tns-platform-declarations": "^6.1.1",
      "typescript": "~3.6.3",
      "prompt": "^1.0.0",
      "rimraf": "^2.6.3"
    },
    "dependencies": {
      "nativescript-appversion": "^1.4.1",
      "@proplugins/nativescript-dom": "2.1.0",
      "@proplugins/nativescript-globalevents": "1.3.0",
      "@proplugins/nativescript-orientation": "2.3.2",
      "raven-js": "^3.27.2"
    }
  }
  
